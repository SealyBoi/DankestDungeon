package dankestdungeon;

import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.FileReader;
import java.io.FileWriter;
import java.util.Scanner;
import java.util.Random;

public class DankestDungeon {

    public static void main(String[] args) {
        //Scanner Creation
        Scanner scan = new Scanner(System.in);
        String sInput = "";
        int intInput = 0;

        //Variable Creations
        boolean running = true;
        boolean mainMenu = true;
        boolean playerCreation = false;
        boolean characterBuild = false;
        boolean easyDungeon = false;
        boolean mediumDungeon = false;
        boolean hardDungeon = false;
        boolean dankestDungeon = false;
        boolean inTown = false;
        boolean inForge = false;
        boolean inShop = false;
        boolean inTraining = false;
