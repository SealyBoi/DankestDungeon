                                System.out.println("You must have mis-typed or some shit because you just stood there like an idiot!");
                        }
                        if (cSpeed >= enemySp) {
                            if (enemyH <= 0) {
                                fight = false;
                            } else {
                                rand = generator.nextInt(100);
                                if (rand <= enemyAcc) {
                                    rand = generator.nextInt(enemyS) + 1;
                                    System.out.println("----------------------------------------------");
                                    System.out.printf("You were hit for %d damage!\n", rand);
                                    cHP = cHP - rand;
                                } else {
                                    System.out.println("----------------------------------------------");
                                    System.out.println("You lucky shit, they missed!");
                                }
                                if (cHP <= 0) {
                                    fight = false;
                                    System.out.println("----------------------------------------------");
                                    System.out.println("The hero falls to the floor, unable to overpower the dank memes presented to him.");
                                    System.out.println("Press enter to end the journey...");
