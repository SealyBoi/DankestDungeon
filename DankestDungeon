                            if (wUCounter == 5) {
                                System.out.println(">Your weapon is max level!");
                            }
                            if (gold >= weaponUpgrade) {
                                cStrength = cStrength + 2;
                                gold = gold - weaponUpgrade;
                                weaponUpgrade = weaponUpgrade + 50;
                                wUCounter++;
                                System.out.printf(">Your strength has risen from %d to %d\n", cStrength - 2, cStrength);
                                System.out.printf(">Weapon level upgraded to level %d", wUCounter);
                            } else {
                                System.out.println(">You do not have enough gold!");
                            }
                            break;
                        case "2":
                            if (aUCounter == 5) {
                                System.out.println(">Your armor is max level!");
                            } else if (gold >= armorUpgrade) {
                                cHealth = cHealth + 3;
                                gold = gold - armorUpgrade;
                                armorUpgrade = armorUpgrade + 50;
                                aUCounter++;
                                System.out.printf(">Your max health has risen from %d to %d\n", cHealth - 3, cHealth);
