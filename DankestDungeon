                                }
                            }
                        }
                        if (enemyH <= 0) {
                            fight = false;
                        }
                    }
                    if (roomCounter < 9) {
                        rand = generator.nextInt(250) + 100;
                        gold += rand;
                        roomCounter++;
                        System.out.println("");
                        System.out.println("");
                        System.out.println("");
                        System.out.println("");
                        System.out.println("");
                        System.out.println("----------------------------------------------");
                        System.out.printf("+%d gold\n", rand);
                        System.out.println("The battle is over. Would you like to continue deeper?");
                        System.out.println("1: Continue to the next room");
                        System.out.println("2: Return to Town");
                        sInput = scan.nextLine();

                        switch (sInput) {
                            case "1":
                                //Continue
                                break;
                            case "2":
                                inTown = true;
                                dankestDungeon = false;
                                continue GAME;
                        }
                    } else {
                        gold += 1000000;
                        System.out.println("");
                        System.out.println("");
                        System.out.println("");
                        System.out.println("");
                        System.out.println("");
                        System.out.println("----------------------------------------------");
                        System.out.println("+1,000,000 gold");
                        System.out.println(">You have completed the fourth floor!");
                        System.out.println(">Press enter to return to town...");
                        sInput = scan.nextLine();
                        endGame = true;
                        dankestDungeon = false;
                        completeDank = "Complete";
                        continue GAME;
                    }
                }

                ENDGAME:
                while (endGame) {
                    fight = true;
                    String currentMonster = "Lord Dankness the III";
                    int enemyS = 46, enemySp = 32, enemyH = 104, enemyAcc = 69;
                    System.out.println("");
                    System.out.println("");
                    System.out.println("");
                    System.out.println("");
                    System.out.println("");
                    System.out.println("----------------------------------------------");
                    System.out.println(">Room *&^$%^%");
                    System.out.printf(">SURPRISE BITCH! You have encountered %s!\n", currentMonster);
                    while (fight) {
                        if (cSpeed < enemySp) {
                            if (enemyH <= 0) {
                                fight = false;
                            } else {
                                System.out.println("----------------------------------------------");
                                System.out.println(">Lord Dankness the III was faster than you and got the upper hand!");
                                rand = generator.nextInt(100);
                                if (rand <= enemyAcc) {
                                    rand = generator.nextInt(enemyS) + 1;
                                    System.out.printf(">You were hit for %d damage!\n", rand);
