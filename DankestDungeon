                    System.out.println("2: Train for +10 Acc (45)");
                    System.out.println("3: Train for +15 Acc (70)");
                    System.out.println("Speed:");
                    System.out.println("4: Train for +1 Speed (25)");
                    System.out.println("5: Train for +2 Speed (45)");
                    System.out.println("6: Train for +3 Speed (70)");
                    System.out.println("");
                    System.out.println("7: Return to Town");
                    sInput = scan.nextLine();

                    switch (sInput) {
                        case "1":
                            if (cAcc == 100) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">Your Acc is already at max!");
                            } else if (gold < 25) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">You do not have enough gold!");
                            } else {
                                gold = gold - 25;
                                if (cAcc + 5 > 100) {
                                    System.out.println("----------------------------------------------");
                                    System.out.printf(">Your accuracy has increased from %d to 100!\n", cAcc);
                                    cAcc = 100;
                                } else {
                                    cAcc = cAcc + 5;
                                    System.out.println("----------------------------------------------");
                                    System.out.printf(">Your accuracy has increased from %d to %d!\n", cAcc - 5, cAcc);
                                }
                            }
                            break;
                        case "2":
                            if (cAcc == 100) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">Your Acc is already at max!");
                            } else if (gold < 45) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">You do not have enough gold!");
                            } else {
                                gold = gold - 45;
                                if (cAcc + 10 > 100) {
                                    System.out.println("----------------------------------------------");
                                    System.out.printf(">Your accuracy has increased from %d to 100!\n", cAcc);
                                    cAcc = 100;
                                } else {
                                    cAcc = cAcc + 10;
                                    System.out.println("----------------------------------------------");
                                    System.out.printf(">Your accuracy has increased from %d to %d!\n", cAcc - 10, cAcc);
                                }
                            }
                            break;
                        case "3":
                            if (cAcc == 100) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">Your Acc is already at max!");
                            } else if (gold < 70) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">You do not have enough gold!");
                            } else {
                                gold = gold - 70;
                                if (cAcc + 15 > 100) {
                                    System.out.println("----------------------------------------------");
                                    System.out.printf(">Your accuracy has increased from %d to 100!\n", cAcc);
                                    cAcc = 100;
                                } else {
                                    cAcc = cAcc + 15;
                                    System.out.println("----------------------------------------------");
                                    System.out.printf(">Your accuracy has increased from %d to %d!\n", cAcc - 15, cAcc);
                                }
                            }
                            break;
                        case "4":
                            if (cSpeed == 32) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">Your Speed is already at max!");
                            } else if (gold < 25) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">You do not have enough gold!");
                            } else {
                                gold = gold - 25;
                                cSpeed = cSpeed + 1;
                                System.out.println("----------------------------------------------");
                                System.out.printf(">Your speed has increased from %d to %d!\n", cSpeed - 1, cSpeed);
                            }
                            break;
                        case "5":
                            if (cSpeed == 32) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">Your Speed is already at max!");
                            } else if (gold < 45) {
                                System.out.println("----------------------------------------------");
                                System.out.println(">You do not have enough gold!");
                            } else {
                                gold = gold - 45;
                                if (cSpeed + 2 > 100) {
                                    System.out.println("----------------------------------------------");
                                    System.out.printf(">Your speed has increased from %d to 32!\n", cSpeed);
                                    cSpeed = 100;
                                } else {
                                    cSpeed = cSpeed + 2;
                                    System.out.println("----------------------------------------------");
                                    System.out.printf(">Your speed has increased from %d to %d!\n", cSpeed - 2, cSpeed);
                                }
                            }
                            break;
