                            if (wUCounter == 5 && wULevel == 0) {
                                System.out.println("1. (150): Heavy Wooden Club");
                            }
                            if (aUCounter == 5 && aULevel == 0) {
                                System.out.println("2. (150): Old Robes");
                            }
