                            }
                        }
                        System.out.println("----------------------------------------------");
                        System.out.printf(">%s's Health: %d\n", currentMonster, enemyH);
                        System.out.printf(">Your Health: %s\n", cHP);
                        System.out.println("What would you like to do?");
                        System.out.printf("1: Basic Attack (%d percent chance of hitting)\n", cAcc);
                        System.out.printf("2: Heavy Attack (%d percent chance of hitting)\n", cAcc - 15);

                        switch (build) {
                            case "1":
                                System.out.printf("3: Holy Smite (%d percent chance of hitting)\n", cAcc - 7);
                                break;
                            case "2":
                                System.out.printf("3: Quick Shot (%d percent chance of hitting)\n", cAcc - 7);
                                break;
                            case "3":
                                System.out.printf("3: Skull Crack (%d percent chance of hitting)\n", cAcc - 7);
                                break;
                            case "4":
                                System.out.printf("3: Quick Heal (%d percent chance of casting)\n", cAcc - 7);
                                break;
                            case "5":
                                System.out.printf("3: Fuck You Kid (%d percent chance of casting)\n", cAcc);
                                break;
                        }

                        System.out.println("4: Run away (like a baby back bitch)");
                        sInput = scan.nextLine();

                        switch (sInput) {
                            case "1":
                                rand = generator.nextInt(100);
                                if (rand <= cAcc) {
                                    dmg = generator.nextInt(cStrength / 2) + cStrength / 2;
                                    rand = generator.nextInt(10);
                                    if (rand == 0) {
                                        System.out.println("----------------------------------------------");
                                        System.out.println("You hit a crit!");
                                        dmg = dmg * 2;
                                    }
                                    enemyH = enemyH - dmg;
                                    System.out.println("----------------------------------------------");
                                    System.out.printf("You hit the enemy for %d damage!\n", dmg);
                                } else {
                                    System.out.println("----------------------------------------------");
                                    System.out.println("Your attack missed!");
                                }
                                break;
                            case "2":
                                rand = generator.nextInt(100);
                                if (rand <= cAcc - 15) {
                                    dmg = generator.nextInt(cStrength / 2) + cStrength / 2 + 5;
                                    rand = generator.nextInt(10);
                                    if (rand == 0) {
                                        System.out.println("----------------------------------------------");
                                        System.out.println("You hit a crit!");
                                        dmg = dmg * 2;
                                    }
                                    enemyH = enemyH - dmg;
                                    System.out.println("----------------------------------------------");
                                    System.out.printf("You hit the enemy for %d damage!\n", dmg);
                                } else {
                                    System.out.println("----------------------------------------------");
                                    System.out.println("Your attack missed!");
                                }
                                break;
                            case "3":
                                rand = generator.nextInt(100);
                                if (build.equals("4")) {
                                    if (rand <= cAcc - 3) {
                                        dmg = generator.nextInt(cStrength / 2) + cStrength / 2 + 5;
                                        rand = generator.nextInt(10);
                                        if (rand == 0) {
                                            System.out.println("----------------------------------------------");
                                            System.out.println("You hit a crit!");
                                            dmg = dmg * 2;
                                        }
                                        cHP = cHP + dmg;
                                        System.out.println("----------------------------------------------");
                                        System.out.printf("You healed yourself for %d health!!\n", dmg);
                                    } else {
                                        System.out.println("----------------------------------------------");
                                        System.out.println("Your spell fizzled!");
                                    }
                                } else if (rand <= cAcc - 7) {
                                    dmg = generator.nextInt(cStrength / 2) + cStrength / 2 + 5;
                                    rand = generator.nextInt(10);
                                    if (rand == 0) {
                                        System.out.println("----------------------------------------------");
                                        System.out.println("You hit a crit!");
                                        dmg = dmg * 2;
                                    }
                                    enemyH = enemyH - dmg;
                                    System.out.println("----------------------------------------------");
                                    System.out.printf("You hit the enemy for %d damage!\n", dmg);
                                } else {
                                    System.out.println("----------------------------------------------");
                                    System.out.println("Your attack missed!");
                                }
                                break;
                            case "4":
                                rand = generator.nextInt(2);
                                if (rand == 0) {
                                    System.out.println("----------------------------------------------");
                                    System.out.println("You failed to run away??? HOW?! It's running!");
                                } else {
                                    System.out.println("----------------------------------------------");
                                    System.out.println("You pussied out and ran away like a baby back bitch. Congrats.");
                                    fight = false;
                                    inTown = true;
                                    endGame = false;
                                    continue GAME;
                                }
                                break;
                            default:
                                System.out.println("----------------------------------------------");
                                System.out.println("You must have mis-typed or some shit because you just stood there like an idiot!");
                        }
                        if (cSpeed >= enemySp) {
                            if (enemyH <= 0) {
                                fight = false;
                            } else {
