                            if (wUCounter == 5 && wULevel == 0) {
                                System.out.println("1. (150): Two-Handed Greataxe");
                            }
                            if (aUCounter == 5 && aULevel == 0) {
                                System.out.println("2. (150): Mercenary's Garbs");
                            }
                            if (wUCounter == 5 && wULevel == 1) {
                                System.out.println("1. (300): Sharpened Greataxe");
                            }
                            if (aUCounter == 5 && aULevel == 1) {
                                System.out.println("2. (300): Raider's Armor");
                            }
                            if (wUCounter == 5 && wULevel == 2) {
                                System.out.println("1. (450): Orc's Greataxe");
