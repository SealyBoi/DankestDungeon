                                    fight = false;
                                    System.out.println("----------------------------------------------");
                                    System.out.println("The hero falls to the floor, unable to overpower the dank memes presented to him.");
                                    System.out.println("Press enter to end the journey...");
                                    sInput = scan.nextLine();

                                    try {
                                        BufferedWriter bw = new BufferedWriter(new FileWriter("save.txt"));

                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
                                        bw.write("");
                                        bw.newLine();
